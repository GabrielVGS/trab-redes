   "source": [
    "import pandas as pd\n",
    "\n",
    "df = pd.read_csv(\"csvs/probes.csv\")\n",
    "df.columns = [\"country\",\"probe_id\",\"ping\"]\n",
    "df[\"probe_id\"] = df.probe_id.str.split(n=1, expand=True)[1].astype(int)\n",
    "df[\"ping\"] = df[\"ping\"].apply(lambda x: 1 if x == \"funciona\" else 0)\n",
    "\n",
    "\n",
    "df.to_csv(\"csvs/probes.csv\")"
   ]
  }
